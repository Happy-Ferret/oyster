{
  "manifest_version": 2,

  "name": "passd",
  "description": "",
  "version": "0.0.0",

  "permissions": [
    "http://localhost:45566/",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-1.10.2.min.js", "content.js"]
  }]
}
